<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#F5FFFA">
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Caesar"
        android:id="@+id/caesar1"
        android:layout_marginTop="34dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="46dp"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KeyWord"
        android:id="@+id/keyword1"
        android:layout_marginEnd="50dp"
        android:layout_alignTop="@+id/caesar1"
        android:layout_alignParentEnd="true"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vigenere"
        android:id="@+id/vigenere1"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/caesar1"
        android:layout_alignStart="@+id/caesar1"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AutoKey"
        android:id="@+id/autokey1"
        android:layout_alignBottom="@+id/vigenere1"
        android:layout_alignStart="@+id/keyword1"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Playfair"
        android:id="@+id/playfair1"
        android:layout_below="@+id/vigenere1"
        android:layout_alignStart="@+id/vigenere1"
        android:layout_marginTop="39dp"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="置换"
        android:id="@+id/permutation1"
        android:layout_alignBottom="@+id/playfair1"
        android:layout_alignStart="@+id/autokey1"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="列置换"
        android:id="@+id/columnpermutation1"
        android:layout_below="@+id/playfair1"
        android:layout_alignStart="@+id/playfair1"
        android:layout_marginTop="40dp"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CA"
        android:id="@+id/ca1"
        android:layout_alignBottom="@+id/columnpermutation1"
        android:layout_alignStart="@+id/permutation1"
        android:background="@drawable/shape"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DES"
        android:id="@+id/des1"
        android:layout_below="@+id/columnpermutation1"
        android:layout_alignStart="@+id/columnpermutation1"
        android:layout_marginTop="39dp"
        android:background="@drawable/shape"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Socket"
        android:id="@+id/socketText"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="返回"
        android:id="@+id/back"
        android:background="@drawable/shape"
        android:layout_above="@+id/socketText"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DH"
        android:id="@+id/DH"
        android:layout_alignTop="@+id/des1"
        android:background="@drawable/shape"
        android:layout_alignStart="@+id/ca1" />

</RelativeLayout>
